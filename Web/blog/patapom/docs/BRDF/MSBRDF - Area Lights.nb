Cell[BoxData[
 RowBox[{
  RowBox[{"LoadLTC", "=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "filename", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"s", "=", 
       RowBox[{"OpenRead", "[", 
        RowBox[{"filename", ",", 
         RowBox[{"BinaryFormat", "\[Rule]", "True"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"count", "=", 
       RowBox[{"BinaryRead", "[", 
        RowBox[{"s", ",", "\"\<Integer32\>\""}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"temp", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"BinaryRead", "[", 
          RowBox[{"s", ",", 
           RowBox[{"{", " ", "\[IndentingNewLine]", " ", 
            RowBox[{
            "\"\<Real64\>\"", "\[IndentingNewLine]", ",", " ", 
             "\"\<Real64\>\"", "\[IndentingNewLine]", ",", " ", 
             "\"\<Real64\>\"", ",", " ", "\"\<Real64\>\"", ",", " ", 
             "\"\<Real64\>\"", ",", " ", "\"\<Real64\>\""}], "}"}]}], 
          "\[IndentingNewLine]", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "count"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Close", "[", "s", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"l", "=", 
       RowBox[{"Length", "[", "temp", "]"}]}], ";", "\[IndentingNewLine]", 
      "temp"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
