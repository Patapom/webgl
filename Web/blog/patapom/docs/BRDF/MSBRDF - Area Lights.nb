       RowBox[{"\[Pi]", "-", 
        RowBox[{
         SubscriptBox["E", "avg"], "(", "\[Alpha]", ")"}]}]]}], 
     RowBox[{
      SubscriptBox["\[Integral]", 
       SubscriptBox["\[CapitalOmega]", "0"]], " ", 
      RowBox[{
       RowBox[{
        SubscriptBox["D", "0"], "(", 
        RowBox[{
         StyleBox[
          SubscriptBox["\[Omega]", "0"],
