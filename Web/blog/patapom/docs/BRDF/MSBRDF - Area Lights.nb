     StyleBox[
      SubscriptBox["\[Omega]", "o"],
      FontWeight->"Bold"], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Integral]", "\[CapitalOmega]"], 
     RowBox[{
      RowBox[{"L", "(", 
       StyleBox[
        SubscriptBox["\[Omega]", "i"],
        FontWeight->"Bold"], ")"}], "  ", 
      RowBox[{
       SubscriptBox["f", "ms"], "(", " ", 
       RowBox[{
        StyleBox[
         SubscriptBox["\[Omega]", "o"],
         FontWeight->"Bold"], 
        StyleBox[",",
         FontWeight->"Bold"], 
        StyleBox[
         SubscriptBox["\[Omega]", "i"],
         FontWeight->"Bold"], ",", "\[Alpha]"}], ")"}], " ", 
      RowBox[{
       StyleBox["(",
        FontWeight->"Plain"], 
       StyleBox[
        RowBox[{
         SubscriptBox["\[Omega]", "i"], " ", "\[CenterDot]", " ", "n"}],
        FontWeight->"Bold"], ")"}], 
      StyleBox[" ",
       FontWeight->"Bold"], 
      RowBox[{"\[DifferentialD]", 
       SubscriptBox["\[Omega]", "i"]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\[LineSeparator]\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"L", "(", 
