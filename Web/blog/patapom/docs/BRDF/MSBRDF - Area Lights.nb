      FontWeight->"Bold"], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Integral]", "\[CapitalOmega]"], 
     RowBox[{
      RowBox[{"L", "(", 
       StyleBox[
        SubscriptBox["\[Omega]", "i"],
        FontWeight->"Bold"], ")"}], "  ", 
      FractionBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"E", "(", 
           RowBox[{
            StyleBox[
             SubscriptBox["\[Omega]", "o"],
             FontWeight->"Bold"], ",", "\[Alpha]"}], ")"}]}], ")"}], 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"E", "(", 
           RowBox[{
            StyleBox[
             SubscriptBox["\[Omega]", "i"],
             FontWeight->"Bold"], ",", "\[Alpha]"}], ")"}]}], ")"}]}], 
       RowBox[{"\[Pi]", "-", 
