Cell[TextData[{
 "So okay, we can accept a certain amount of correlation that is given by the \
value at ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    SubscriptBox["\[Theta]", "0"], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "when \[Alpha]=1 and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Theta]", "0"], "=", 
    FractionBox["\[Pi]", "2"]}], TraditionalForm]]],
 "\nNow we can find the roughness for a given ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Theta]", "0"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " :\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"c", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox[
       RowBox[{
        RowBox[{"ndf", "(", 
         RowBox[{
          FractionBox[
           SubscriptBox["\[Theta]", "0"], "2"], ",", "\[Alpha]"}], ")"}], " ", 
        RowBox[{"cos", "(", 
         FractionBox[
          SubscriptBox["\[Theta]", "0"], "2"], ")"}], 
        RowBox[{"sin", "(", 
         FractionBox[
